  "license": "ISC",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "3.1.4",
    "aws-sdk": "2.730.0",
    "bcrypt": "5.0.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "concurrently": "5.2.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fs": "0.0.1-security",
    "https": "1.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.19",
    "mongoose": "5.9.22",
    "multer": "1.4.2",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "validator": "13.1.1"
  },
  "devDependencies": {
    "nodemon": "2.0.4"
  }
